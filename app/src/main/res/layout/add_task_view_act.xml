<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#202020"
    android:backgroundTint="#46353535"
    android:backgroundTintMode="screen"
    tools:context=".AddTaskView">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="261dp"
        android:layout_height="57dp"
        android:fontFamily="@font/fredokaone_regular"
        android:text="Create New Task"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="198dp"
        android:layout_height="71dp"
        android:fontFamily="@font/fredokaone_regular"
        android:text="Submit"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="424dp"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" >

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/view_and_add_task_background" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/tag_for_add_task_view4"
                    android:layout_width="179dp"
                    android:layout_height="43dp"
                    android:background="@android:color/white"
                    android:foreground="@android:color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.081"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.456"
                    app:srcCompat="@drawable/text_box_1" />

                <Spinner
                    android:id="@+id/spinner2"
                    android:layout_width="116dp"
                    android:layout_height="36dp"
                    android:textAlignment="gravity"
                    app:layout_constraintBottom_toBottomOf="@+id/tag_for_add_task_view4"
                    app:layout_constraintEnd_toEndOf="@+id/tag_for_add_task_view4"
                    app:layout_constraintStart_toEndOf="@+id/name_for_add_task_view_text11"
                    app:layout_constraintTop_toTopOf="@+id/tag_for_add_task_view4" />

                <TextView
                    android:id="@+id/name_for_add_task_view_text11"
                    android:layout_width="44dp"
                    android:layout_height="28dp"
                    android:fontFamily="@font/fredokaone_regular"
                    android:text="Tag:"
                    android:textColor="@color/teal_200"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tag_for_add_task_view4"
                    app:layout_constraintEnd_toEndOf="@+id/tag_for_add_task_view4"
                    app:layout_constraintHorizontal_bias="0.066"
                    app:layout_constraintStart_toStartOf="@+id/tag_for_add_task_view4"
                    app:layout_constraintTop_toTopOf="@+id/tag_for_add_task_view4"
                    app:layout_constraintVertical_bias="0.533" />

                <ImageView
                    android:id="@+id/name_for_add_task_view"
                    android:layout_width="378dp"
                    android:layout_height="67dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    app:layout_constraintBottom_toTopOf="@+id/time_for_add_task_view"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.484"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/text_box_1" />

                <TextView
                    android:id="@+id/name_for_add_task_view_text7"
                    android:layout_width="109dp"
                    android:layout_height="47dp"
                    android:fontFamily="@font/fredokaone_regular"
                    android:text="Name: "
                    android:textColor="@android:color/white"
                    android:textSize="25sp"
                    app:layout_constraintBottom_toBottomOf="@+id/name_for_add_task_view"
                    app:layout_constraintEnd_toEndOf="@+id/name_for_add_task_view"
                    app:layout_constraintHorizontal_bias="0.152"
                    app:layout_constraintStart_toStartOf="@+id/name_for_add_task_view"
                    app:layout_constraintTop_toTopOf="@+id/name_for_add_task_view"
                    app:layout_constraintVertical_bias="0.45" />


                <EditText
                    android:id="@+id/name_input2"
                    android:layout_width="195dp"
                    android:layout_height="46dp"
                    android:layout_marginStart="4dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="Task Name"
                    android:textColor="#88FFFFFF"
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="@+id/name_for_add_task_view"
                    app:layout_constraintEnd_toEndOf="@+id/name_for_add_task_view"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toEndOf="@+id/name_for_add_task_view_text7"
                    app:layout_constraintTop_toTopOf="@+id/name_for_add_task_view"
                    app:layout_constraintVertical_bias="0.476" />


                <ImageView
                    android:id="@+id/time_for_add_task_view"
                    android:layout_width="396dp"
                    android:layout_height="79dp"
                    android:scaleY="0.7"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.515"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.274"
                    app:srcCompat="@drawable/text_box_1" />

                <TextView
                    android:id="@+id/name_for_add_task_view_text9"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:fontFamily="@font/fredokaone_regular"
                    android:text="End Time: "
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toBottomOf="@+id/time_for_add_task_view"
                    app:layout_constraintEnd_toStartOf="@+id/number_picker_end"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/number_picker_start"
                    app:layout_constraintTop_toTopOf="@+id/time_for_add_task_view"
                    app:layout_constraintVertical_bias="0.489" />


                <NumberPicker
                    android:id="@+id/number_picker_end"
                    android:layout_width="85dp"
                    android:layout_height="42dp"
                    android:ems="10"
                    android:inputType="time"
                    android:background="@android:color/white"
                    app:layout_constraintBottom_toBottomOf="@+id/time_for_add_task_view"
                    app:layout_constraintEnd_toStartOf="@+id/time_for_add_task_view"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/name_for_add_task_view_text9"
                    app:layout_constraintTop_toTopOf="@+id/time_for_add_task_view"
                    app:layout_constraintVertical_bias="0.513" />

                <NumberPicker
                    android:id="@+id/number_picker_start"
                    android:layout_width="96dp"
                    android:layout_height="41dp"
                    android:background="@android:color/white"
                    android:ems="10"
                    android:inputType="time"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@+id/time_for_add_task_view"
                    app:layout_constraintEnd_toStartOf="@+id/name_for_add_task_view_text9"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/name_for_add_task_view_text8"
                    app:layout_constraintTop_toTopOf="@+id/time_for_add_task_view"
                    app:layout_constraintVertical_bias="0.454" />

                <TextView
                    android:id="@+id/name_for_add_task_view_text8"
                    android:layout_width="101dp"
                    android:layout_height="31dp"
                    android:fontFamily="@font/fredokaone_regular"
                    android:text="Start Time: "
                    android:textColor="@android:color/holo_purple"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toTopOf="@+id/tag_for_add_task_view4"
                    app:layout_constraintEnd_toStartOf="@+id/number_picker_start"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/time_for_add_task_view"
                    app:layout_constraintTop_toTopOf="@+id/time_for_add_task_view" />

                <ImageView
                    android:id="@+id/tag_for_add_task_view3"
                    android:layout_width="180dp"
                    android:layout_height="90dp"
                    android:scaleY="1.2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.948"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.467"
                    app:srcCompat="@drawable/text_box_1" />

                <TextView
                    android:id="@+id/name_for_add_task_view_text12"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:fontFamily="@font/fredokaone_regular"
                    android:text="Due Date:"
                    android:textColor="?attr/colorError"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toBottomOf="@+id/tag_for_add_task_view3"
                    app:layout_constraintEnd_toEndOf="@+id/tag_for_add_task_view3"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/tag_for_add_task_view3"
                    app:layout_constraintTop_toTopOf="@+id/tag_for_add_task_view3"
                    app:layout_constraintVertical_bias="0.492" />

                <EditText
                    android:id="@+id/due_date_text2"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:text="(Date)"
                    android:textColor="#88FFFFFF"
                    android:textSize="12dp"
                    app:layout_constraintBottom_toBottomOf="@+id/tag_for_add_task_view3"
                    app:layout_constraintEnd_toEndOf="@+id/tag_for_add_task_view3"
                    app:layout_constraintStart_toEndOf="@+id/name_for_add_task_view_text12"
                    app:layout_constraintTop_toTopOf="@+id/tag_for_add_task_view3" />

                <ImageView
                    android:id="@+id/description_block_view_task"
                    android:layout_width="378dp"
                    android:layout_height="67dp"
                    android:scaleY="2.5"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.575"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.792"
                    app:srcCompat="@drawable/text_box_1" />

                <TextView
                    android:id="@+id/name_for_add_task_view_text13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/fredokaone_regular"
                    android:text="Description:"
                    android:textColor="@android:color/white"
                    android:textSize="25sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/description_block_view_task"
                    app:layout_constraintHorizontal_bias="0.487"
                    app:layout_constraintStart_toStartOf="@+id/description_block_view_task"
                    app:layout_constraintTop_toBottomOf="@+id/time_for_add_task_view"
                    app:layout_constraintVertical_bias="0.283" />

                <EditText
                    android:id="@+id/description_input2"
                    android:layout_width="309dp"
                    android:layout_height="132dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="(This is description of my new task)"
                    android:textColor="#88FFFFFF"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/time_for_add_task_view"
                    app:layout_constraintVertical_bias="0.61" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </FrameLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>