<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout3"
    android:layout_width="fill_parent"
    android:layout_height="200dp"
    android:background="@color/black"
    android:padding="6dip">


    <ImageView
        android:id="@+id/task_background"
        android:layout_width="404dp"
        android:layout_height="186dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/task_box" />

    <TextView
        android:id="@+id/task_list_due_date_text"
        android:layout_width="82dp"
        android:layout_height="42dp"
        android:fontFamily="@font/fredokaone_regular"
        android:gravity="center"
        android:maxLines="1"
        android:text="Due Date"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/task_list_start"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread" />

    <TextView
        android:id="@+id/task_list_description"
        android:layout_width="173dp"
        android:layout_height="88dp"
        android:layout_marginStart="4dp"
        android:fontFamily="@font/fredokaone_regular"
        android:maxLength="100"
        android:maxLines="4"
        android:text="Description"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/task_list_end"
        app:layout_constraintHorizontal_bias="0.252"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/task_list_name" />

    <TextView
        android:id="@+id/task_list_due_date"
        android:layout_width="77dp"
        android:layout_height="38dp"
        android:layout_alignWithParentIfMissing="true"
        android:fontFamily="@font/fredokaone_regular"
        android:gravity="center_vertical"
        android:text="Due Date:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/task_list_ends_at"
        app:layout_constraintEnd_toStartOf="@+id/task_list_due_date_text"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/task_list_name"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/task_list_name"
        android:layout_width="173dp"
        android:layout_height="52dp"
        android:layout_alignWithParentIfMissing="true"
        android:fontFamily="@font/fredokaone_regular"
        android:text="Name"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:maxLines="2"
        app:layout_constraintBottom_toTopOf="@+id/task_list_description"
        app:layout_constraintEnd_toStartOf="@+id/task_list_start"
        app:layout_constraintHorizontal_bias="0.278"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/task_list_ends_at"
        android:layout_width="83dp"
        android:layout_height="43dp"
        android:layout_marginEnd="4dp"
        android:fontFamily="@font/fredokaone_regular"
        android:gravity="center"
        android:maxLines="1"
        android:text="Starts at:"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/task_list_starts_at"
        app:layout_constraintEnd_toStartOf="@+id/task_list_start"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/task_list_due_date"
        app:layout_constraintVertical_chainStyle="packed" />

    <TextView
        android:id="@+id/task_list_starts_at"
        android:layout_width="69dp"
        android:layout_height="49dp"
        android:fontFamily="@font/fredokaone_regular"
        android:gravity="center"
        android:maxLines="1"
        android:text="Ends at:"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/task_list_end"
        app:layout_constraintHorizontal_bias="0.347"
        app:layout_constraintStart_toEndOf="@+id/task_list_description"
        app:layout_constraintTop_toBottomOf="@+id/task_list_ends_at" />

    <TextView
        android:id="@+id/task_list_start"
        android:layout_width="67dp"
        android:layout_height="43dp"
        android:gravity="center"
        android:maxLines="1"
        android:text="Start Time"
        android:textSize="15sp"
        android:fontFamily="@font/fredokaone_regular"
        app:layout_constraintBottom_toTopOf="@+id/task_list_end"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.903"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/task_list_due_date_text"
        app:layout_constraintVertical_chainStyle="packed" />

    <TextView
        android:id="@+id/task_list_end"
        android:layout_width="68dp"
        android:layout_height="51dp"
        android:gravity="center"
        android:maxLines="1"
        android:text="End"
        android:textSize="15sp"
        android:fontFamily="@font/fredokaone_regular"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.906"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/task_list_start" />

    <TextView
        android:id="@+id/isCompleted"
        android:layout_width="4dp"
        android:layout_height="218dp"
        android:layout_marginStart="372dp"
        android:background="#000000"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="-11dp" />

</androidx.constraintlayout.widget.ConstraintLayout>